{
  "name": "@digital-boss/n8n-nodes-suitecrm",
  "version": "1.0.1",
  "author": "IÃ±aki Breinbauer",
  "license": "MIT",
  "description": "suiteCrm support for n8n",
  "keywords": [
    "n8n",
    "nodemation",
    "dnc",
    "digital north consulting",
    "digital boss",
    "helpdesk",
    "customer service",
    "nodes",
    "custom",
    "suiteCrm",
    "workflow"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quansenB/n8n-nodes-suitecrm.git"
  },
  "main": "index.js",
  "scripts": {
    "dev": "npm run watch",
    "build": "tsc && gulp",
    "lint": "eslint .eslintrc.js --ext .ts",
    "lintfix": "eslint --fix .eslintrc.js --ext .ts",
    "watch": "tsc --watch",
    "test": "jest",
    "release": "npm run build && npm publish"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "credentials": [
      "dist/credentials/SuiteCrmV4Api.credentials.js",
      "dist/credentials/SuiteCrmV8Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SuiteCrm/SuiteCrm.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.4",
    "@types/request": "^2.48.8",
    "@types/request-promise-native": "^1.0.18",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "gulp": "^4.0.2",
    "jest": "^28.1.3",
    "n8n-core": "^0.130.0",
    "n8n-workflow": "^0.112.0",
    "ts-jest": "^28.0.8",
    "typescript": "~4.7.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "bugs": {
    "url": "https://github.com/quansenB/n8n-nodes-suitecrm/issues"
  },
  "homepage": "https://github.com/quansenB/n8n-nodes-suitecrm#readme"
}
